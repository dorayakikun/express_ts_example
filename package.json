{
  "name": "express_ts_example",
  "version": "0.1.0",
  "description": "A project template for the Express with TypeScript.",
  "main": "dist/server.js",
  "types": "dist/server.d.ts",
  "scripts": {
    "build": "tsc",
    "fix": "eslint --fix --ext .ts .",
    "lint": "eslint --ext .ts .",
    "serve": "node dist/server.js",
    "start": "yarn build && yarn serve",
    "test": "jest",
    "typecheck": "tsc -p . --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/dorayakikun/express_ts_example.git"
  },
  "keywords": [
    "Express",
    "TypeScript"
  ],
  "author": "dorayakikun <amakunai.dorayaki@gmaill.com> (https://medium.com/@dorayakikun)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dorayakikun/express_ts_example/issues"
  },
  "homepage": "https://github.com/dorayakikun/express_ts_example#readme",
  "dependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/compression": "0.0.36",
    "@types/errorhandler": "0.0.32",
    "@types/express": "^4.16.0",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "errorhandler": "^1.5.0",
    "express": "^4.16.3"
  },
  "devDependencies": {
    "@types/jest": "^24.0.0",
    "@types/supertest": "^2.0.7",
    "@typescript-eslint/eslint-plugin": "^1.5.0",
    "@typescript-eslint/parser": "^1.5.0",
    "@typescript-eslint/typescript-estree": "^1.5.0",
    "eslint": "^5.6.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "jest": "^23.6.0",
    "prettier": "^1.14.2",
    "supertest": "^3.4.1",
    "ts-jest": "^24.0.0",
    "typescript": "^3.0.3",
    "typescript-eslint-parser": "^21.0.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testURL": "http://localhost/",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$"
  }
}
